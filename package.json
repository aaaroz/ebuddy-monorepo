{
  "name": "ebuddy-test-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\" ",
    "dev:backend": "turbo run backend-repo#dev",
    "dev:frontend": "turbo run frontend-repo#dev"
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "turbo": "^2.4.0",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@mui/icons-material": "^6.4.3",
    "zod": "^3.24.1"
  }
}
